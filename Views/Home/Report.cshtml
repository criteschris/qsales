@model qsales.Models.ReportPageViewModel

@{
    ViewData["Title"] = "Report";
}

<h2>@(User.Identity.Name)</h2>
<h3>Is in MyAdminRole? @(User.IsInRole("MyAdminRole"))</h3>
<h3>Is in MyOAdminGroup? @(User.IsInRole("MyOAdminGroup"))</h3>
<table class="table">
@foreach(var claim in User.Claims)
{
    <tr>
        <td>@claim.Type</td>
        <td>@claim.Value</td>
    </tr>
}
</table>

<app redux-initial-state="Model" container-element-id="react-app"></app>

@section scripts {
    <script src="~/js/report.js" asp-append-version="true"></script>
}